{
	"name": "vno-camelcase",
	"displayName": "vno-camelcase",
	"description": "Change selected text to camel-cased.",
	"version": "1.1.0",
	"engines": {
		"vscode": "^1.36.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.ccase"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.ccase",
				"title": "Camel Case: camelCase"
			},
			{
				"command": "extension.scase",
				"title": "Snake Case: snakeCase"
			}
		]
	},
	"scripts": {
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.36.0",
		"eslint": "^5.13.0",
		"glob": "^7.1.4",
		"mocha": "^8.2.1",
		"typescript": "^3.3.1",
		"vscode-test": "^1.2.0"
	},
	"publisher": "xvno",
	"repository": {
		"xvno": "https://github.com/xvno/vscode-vno-camelcase.git"
	}
}
